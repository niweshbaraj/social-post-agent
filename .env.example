# X (Twitter) API Credentials
X_ACCESS_TOKEN=your_twitter_access_token
X_ACCESS_TOKEN_SECRET=your_twitter_access_token_secret
X_API_KEY=your_twitter_api_key
X_API_SECRET=your_twitter_api_secret

# LinkedIn API Credentials
LINKEDIN_ACCESS_TOKEN=your_linkedin_access_token
LINKEDIN_PERSON_ID=your_linkedin_person_id

# Instagram API Credentials (Facebook Graph API)
INSTAGRAM_ACCESS_TOKEN=your_instagram_access_token
INSTAGRAM_BUSINESS_ACCOUNT_ID=your_instagram_business_account_id

# LangSmith API Keys (for observability and monitoring)
LANGCHAIN_API_KEY=your_langchain_api_key_here
LANGCHAIN_TRACING_V2=true
LANGCHAIN_PROJECT=social-post-agent
LANGCHAIN_ENDPOINT=https://api.smith.langchain.com

# OpenAI API Key (for content generation)
OPENAI_API_KEY=your_openai_api_key

# Together AI API Key (for content generation)
# This is used for the Together AI model, which is a part of the content generation process
# Ensure you have the Together AI API key set up in your environment
# You can obtain this key from the Together AI platform after creating an account
TOGETHER_API_KEY=your_together_api_key

# Hugging Face API Key (for image generation)
HUGGINGFACE_API_KEY=your_huggingface_api_key
